FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0.1 0 0);

boundaryField
{
    floor
    {
        type            noSlip;
    }
    ceiling
    {
        type            noSlip;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (0.1 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0.1 0 0);
    }
    fixedWalls
    {
        type            empty;
    }
    baffleFacesThermoBaffle1D_master
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    baffleFacesThermoBaffle1D_slave
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    baffleFacesThermoBaffle3D_master
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    baffleFacesThermoBaffle3D_slave
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


